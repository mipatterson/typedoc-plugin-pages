{
	"parser": "@typescript-eslint/parser",
	"plugins": ["@typescript-eslint"],
	"extends": [
		"plugin:@typescript-eslint/recommended"
	],
	"env": {
		"node": true
	},
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"rules": {
		"@typescript-eslint/no-explicit-any": "off",
		"semi": "error"
	}
}